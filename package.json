{
  "name": "react-ward",
  "version": "0.0.2",
  "description": "React Webpack boilerplate",
  "author": "Ward Oosterlijnck",
  "license": "ISC",
  "keywords": [
    "react",
    "webpack"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --history-api-fallback --open",
    "build": "cross-env NODE_ENV=production webpack",
    "prestart": "npm run build",
    "start": "serve build --single --cache 1",
    "lint:js": "eslint src --fix && flow check",
    "lint:css": "stylefmt --config .stylelintrc --recursive src/**/*.css",
    "lint": "npm run lint:js && npm run lint:css",
    "test": "jest --config=.jestrc.json",
    "test:watch": "npm run test -- --watchAll",
    "precoverage": "npm run test -- --coverage",
    "coverage": "open coverage/index.html"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "clean-webpack-plugin": "0.1.16",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "eslint": "4.2.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-react": "5.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.1.0",
    "eslint-plugin-standard": "3.0.1",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "flow-bin": "0.49.1",
    "html-webpack-plugin": "2.29.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "20.0.4",
    "null-loader": "0.1.1",
    "postcss-cssnext": "3.0.2",
    "postcss-import": "10.0.0",
    "postcss-loader": "2.0.6",
    "postcss-reporter": "4.0.0",
    "script-ext-html-webpack-plugin": "1.8.3",
    "serve": "6.0.2",
    "style-loader": "0.18.2",
    "stylefmt": "6.0.0",
    "stylelint": "7.13.0",
    "stylelint-config-standard": "16.0.0",
    "url-loader": "0.5.9",
    "webpack": "3.2.0",
    "webpack-dev-server": "2.5.1"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "classnames": "2.2.5",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.0",
    "normalize.css": "7.0.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-router-dom": "4.1.1"
  }
}
