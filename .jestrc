{
  "collectCoverageFrom": [
    "src/**/*.{js}"
  ],
  "moduleFileExtensions": ["js"],
  "moduleDirectories": ["src", "node_modules"],
  "moduleNameMapper": {
    "^.+\\.css$": "identity-obj-proxy",
    "^.+\\.(svg|woff2?|jpe?g|png|gif)$": "null-loader"
  },
  "coverageReporters": ["text-summary", "html"]
}
